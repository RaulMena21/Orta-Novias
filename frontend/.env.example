# Analytics Configuration for Orta Novias
# Copy this file to .env.local and update with your actual values

# Google Analytics 4 Measurement ID
# Get this from your Google Analytics property
VITE_GA_MEASUREMENT_ID=G-XXXXXXXXXX

# Analytics Debug Mode (set to true for development)
VITE_ANALYTICS_DEBUG=true

# Analytics Cookie Domain (optional)
VITE_ANALYTICS_COOKIE_DOMAIN=auto

# Enable Analytics in Development (optional)
VITE_ANALYTICS_DEV_MODE=false

# Business Information for Enhanced Analytics
VITE_BUSINESS_TYPE=bridal_boutique
VITE_BUSINESS_NAME=Orta Novias
VITE_BUSINESS_LOCATION=Spain

# Conversion Values (in EUR)
VITE_APPOINTMENT_VALUE=1000
VITE_CONSULTATION_VALUE=500
VITE_DRESS_INQUIRY_VALUE=2000
